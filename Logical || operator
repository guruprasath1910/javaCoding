import java.lang.System;
import java.util.Scanner;

public class Main
{
    public static void main(String[] args)
    {
        Scanner scan = new Scanner(System.in);
        int num = scan.nextInt();
        scan.nextLine();
        
        if(num%3==0 || num%5==0)
        {
            System.out.println("Positive");
        }
        else
        {
            System.out.print("Negative");
        } 
       
    }
}
